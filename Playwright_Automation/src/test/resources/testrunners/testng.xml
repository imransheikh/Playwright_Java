<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Open cart Regression Suite" thread-count= "1" parallel = "tests" verbose="4">
	
	<listeners>
		<listener class-name="com.qa.opencart.listners.ExtentReportListener" />
	</listeners>
	
	<test name="Open cart Home Page Test">
		<classes>
			<class name="com.qa.opencart.tests.HomePageTest" />
		</classes>
	</test> 
	
	<test name="Open cart Login Page Test">
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test> 
</suite> 
